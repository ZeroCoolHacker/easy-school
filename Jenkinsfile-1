pipeline {
  agent any
  stages {
    stage('version') {
      steps {
        sh 'python3 --version'
      }
    }
    stage('Scan') {
      steps {
        withSonarQubeEnv(installationName: 'sonarqube') { 
          sh './mvnw clean org.sonarsource.scanner.maven:sonar-maven-plugin:3.9.0.2155:sonar'
        }
      }
    }
    stage('hello') {
      steps {
        sh 'python3 --version'
      }
    }
  }
}
